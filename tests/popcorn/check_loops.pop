// check_loops.pop

#include "core.h"
open Core;

void pop_main()
{
  int i;

  print_string("for   ... = ");
  for(i=5;i>0;i--)
    {
      print_int(i);
      print_string(" ");
    }
  
  print_string("\nwhile ... = ");
  i=5;
  while(i>0)
    {
      print_int(i);
      print_string(" ");
      i--;
    }
  
  print_string("\ndo    ... = ");
  i=5;
  do 
    {
      print_int(i);
      print_string(" ");
      i--;    
    }
  while (i>0);
  
  print_string("\n");
  return;
}
